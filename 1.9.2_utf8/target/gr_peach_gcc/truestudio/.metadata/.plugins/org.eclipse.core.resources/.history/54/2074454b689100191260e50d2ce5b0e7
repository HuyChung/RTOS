/*
 *  各タスクの優先度の定義: Dinh nghia do uu tien cua Task
 */

#define HIGH_PRIORITY	9		/* 並行実行されるタスクの優先度 */
#define MID_PRIORITY	10
#define LOW_PRIORITY	11

